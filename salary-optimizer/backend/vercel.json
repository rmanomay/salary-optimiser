{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/calculate.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/calculate",
            "dest": "/api/calculate.py"
        },
        {
            "src": "/api/?$",
            "dest": "/api/index.py"
        }
    ]
}